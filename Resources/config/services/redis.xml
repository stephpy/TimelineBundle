    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="spy_timeline.redis.action_manager">Spy\TimelineBundle\Redis\TimelineActionManager</parameter>
    </parameters>

    <services>
        <service id="spy_timeline.timeline_action_manager.default" class="%spy_timeline.redis.action_manager%" public="false">

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="spy_timeline.notification_manager.class">Spy\TimelineBundle\Notification\NotificationManager</parameter>
        <parameter key="spy_timeline.unread_notifications.class">Spy\TimelineBundle\Notification\Unread\UnreadNotificationManager</parameter>
    </parameters>

    <services>
        <service id="spy_timeline.notification_manager" class="%spy_timeline.notification_manager.class%">
        </service>

		<service id="spy_timeline.unread_notifications" class="%spy_timeline.unread_notifications.class%">
            <argument type="service" id="spy_timeline.provider" />
        </service>

    </services>

</container>

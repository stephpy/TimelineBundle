<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="spy_timeline.filter.duplicate_key.class">Spy\TimelineBundle\Filter\DuplicateKey</parameter>
        <parameter key="spy_timeline.filter.data_hydrator.class">Spy\TimelineBundle\Filter\DataHydrator</parameter>
    </parameters>

    <services>
        <service id="spy_timeline.filter.duplicate_key" class="%spy_timeline.filter.duplicate_key.class%" />

        <service id="spy_timeline.filter.data_hydrator" class="%spy_timeline.filter.data_hydrator.class%">
            <argument type="service" id="service_container" />
        </service>
    </services>

</container>

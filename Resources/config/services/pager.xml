<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="spy_timeline.pager.subscriber" class="Spy\TimelineBundle\Pager\Subscriber\TimelineSubscriber">
			<tag name="knp_paginator.subscriber" />
			<argument type="service" id="spy_timeline.manager" />
			<argument type="service" id="spy_timeline.unread_notifications" />
        </service>
    </services>

</container>
